{
    "name": "backend",
    "packageManager": "yarn@3.1.1",
    "scripts": {
        "start": "yarn db:migration:deploy && node ./.output/index.js",
        "build": "tsc --project tsconfig.json --outDir .output --sourceMap false --pretty",
        "typecheck": "tsc --project tsconfig.json --noEmit --pretty",
        "dev": "ts-node-dev --clear --rs --inspect -- index.ts",
        "db:client:gen": "prisma generate",
        "db:schema:format": "prisma format",
        "db:migration:new": "prisma migrate dev --create-only",
        "db:migration:deploy": "prisma migrate deploy",
        "graphql:schema:dump": "ts-node dump-schema.ts",
        "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore"
    },
    "dependencies": {
        "@apollo/server": "^4.4.0",
        "@prisma/client": "^4.10.1",
        "@sentry/node": "^7.21.1",
        "@types/express": "^4.17.14",
        "@types/node": "^18.11.9",
        "argon2": "^0.30.2",
        "axios": "^1.2.0",
        "body-parser": "^1.20.2",
        "class-validator": "^0.13.2",
        "connect-redis": "^6.1.3",
        "content-disposition": "^0.5.4",
        "cookie": "^0.5.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "exceljs": "^4.3.0",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "fast-glob": "^3.2.12",
        "graphql": "^16.6.0",
        "graphql-fields": "^2.0.3",
        "graphql-scalars": "^1.20.1",
        "graphql-upload": "^13.0.0",
        "handlebars": "^4.7.7",
        "html-minifier": "^4.0.0",
        "html-to-text": "^8.2.1",
        "http-status-codes": "^2.2.0",
        "inline-css": "^4.0.1",
        "jsvat": "^2.5.3",
        "lodash": "^4.17.21",
        "mime-types": "^2.1.35",
        "minio": "^7.0.32",
        "nodemailer": "^6.8.0",
        "qrcode": "^1.5.1",
        "rambdax": "^9.0.0",
        "redis": "^4.5.0",
        "reflect-metadata": "^0.1.13",
        "server-timing": "^3.3.2",
        "sharp": "^0.31.2",
        "soap": "^0.45.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.4.1",
        "type-graphql": "2.0.0-beta.1",
        "typescript": "^4.9.3",
        "zod": "^3.19.1"
    },
    "devDependencies": {
        "@types/connect-redis": "^0.0.19",
        "@types/content-disposition": "^0.5.5",
        "@types/cookie": "^0.5.1",
        "@types/cors": "^2.8.13",
        "@types/express-session": "^1.17.5",
        "@types/graphql-fields": "^1.3.4",
        "@types/graphql-upload": "^8.0.12",
        "@types/html-minifier": "^4.0.2",
        "@types/html-to-text": "^8.1.1",
        "@types/inline-css": "^3.0.1",
        "@types/lodash": "^4.14.190",
        "@types/mime-types": "^2.1.1",
        "@types/minio": "^7.0.15",
        "@types/nodemailer": "^6.4.6",
        "@types/qrcode": "^1.5.0",
        "@types/sax": "^1.2.4",
        "@types/sharp": "^0.31.0",
        "@types/validator": "^13.7.10",
        "eslint": "^8.28.0",
        "prisma": "^4.10.1",
        "ts-node-dev": "^2.0.0",
        "type-fest": "^3.2.0",
        "typegraphql-prisma": "^0.24.0"
    }
}

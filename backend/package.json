{
    "name": "backend",
    "packageManager": "yarn@3.1.1",
    "scripts": {
        "start": "yarn db:migration:deploy && node ./.output/index.js",
        "build": "tsc --project tsconfig.json --outDir .output --sourceMap false --pretty",
        "typecheck": "tsc --project tsconfig.json --noEmit --pretty",
        "dev": "ts-node-dev --clear --rs --inspect -- index.ts",
        "db:client:gen": "prisma generate",
        "db:schema:format": "prisma format",
        "db:migration:new": "prisma migrate dev --create-only",
        "db:migration:deploy": "prisma migrate deploy",
        "graphql:schema:dump": "ts-node dump-schema.ts",
        "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore"
    },
    "dependencies": {
        "@apollo/server": "^4.6.0",
        "@prisma/client": "^4.12.0",
        "@sentry/node": "^7.48.0",
        "@types/express": "^4.17.17",
        "@types/node": "^18.15.11",
        "argon2": "^0.30.3",
        "axios": "^1.3.5",
        "body-parser": "^1.20.2",
        "class-validator": "^0.14.0",
        "connect-redis": "^6.1.3",
        "content-disposition": "^0.5.4",
        "cookie": "^0.5.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "exceljs": "^4.3.0",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "fast-glob": "^3.2.12",
        "graphql": "^16.6.0",
        "graphql-fields": "^2.0.3",
        "graphql-scalars": "^1.21.3",
        "graphql-upload": "^13.0.0",
        "handlebars": "^4.7.7",
        "html-minifier": "^4.0.0",
        "html-to-text": "^9.0.5",
        "http-status-codes": "^2.2.0",
        "inline-css": "^4.0.2",
        "jsvat": "^2.5.3",
        "lodash": "^4.17.21",
        "mime-types": "^2.1.35",
        "minio": "^7.0.33",
        "nodemailer": "^6.9.1",
        "qrcode": "^1.5.1",
        "rambdax": "^9.1.0",
        "redis": "^4.6.5",
        "reflect-metadata": "^0.1.13",
        "server-timing": "^3.3.3",
        "sharp": "^0.32.0",
        "soap": "^1.0.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.0",
        "type-graphql": "2.0.0-beta.1",
        "typescript": "^5.0.4",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/connect-redis": "^0.0.20",
        "@types/content-disposition": "^0.5.5",
        "@types/cookie": "^0.5.1",
        "@types/cors": "^2.8.13",
        "@types/express-session": "^1.17.7",
        "@types/graphql-fields": "^1.3.5",
        "@types/graphql-upload": "^8.0.12",
        "@types/html-minifier": "^4.0.2",
        "@types/html-to-text": "^9.0.0",
        "@types/inline-css": "^3.0.1",
        "@types/lodash": "^4.14.194",
        "@types/mime-types": "^2.1.1",
        "@types/minio": "^7.0.18",
        "@types/nodemailer": "^6.4.7",
        "@types/qrcode": "^1.5.0",
        "@types/sax": "^1.2.4",
        "@types/sharp": "^0.31.1",
        "@types/validator": "^13.7.15",
        "eslint": "^8.38.0",
        "prisma": "^4.12.0",
        "ts-node-dev": "^2.0.0",
        "type-fest": "^3.8.0",
        "typegraphql-prisma": "^0.24.6"
    }
}

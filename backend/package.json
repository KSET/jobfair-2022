{
    "name": "backend",
    "packageManager": "yarn@3.1.1",
    "scripts": {
        "start": "yarn db:migration:deploy && node ./.output/index.js",
        "build": "tsc --project tsconfig.json --outDir .output --sourceMap false --pretty",
        "dev": "ts-node-dev --clear --rs --inspect -- index.ts",
        "db:client:gen": "prisma generate",
        "db:schema:format": "prisma format",
        "db:migration:new": "prisma migrate dev --create-only",
        "db:migration:deploy": "prisma migrate deploy",
        "graphql:schema:dump": "ts-node dump-schema.ts",
        "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore"
    },
    "dependencies": {
        "@prisma/client": "^3.12.0",
        "@sentry/node": "^6.19.6",
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.25",
        "apollo-server-express": "^3.6.7",
        "argon2": "^0.28.5",
        "axios": "^0.26.1",
        "class-validator": "^0.13.2",
        "connect-redis": "^6.1.3",
        "content-disposition": "^0.5.4",
        "cookie": "^0.5.0",
        "dotenv": "^16.0.0",
        "exceljs": "^4.3.0",
        "express": "^4.17.3",
        "express-session": "^1.17.2",
        "fast-glob": "^3.2.11",
        "graphql": "^15.8.0",
        "graphql-fields": "^2.0.3",
        "graphql-upload": "^13.0.0",
        "handlebars": "^4.7.7",
        "html-minifier": "^4.0.0",
        "http-status-codes": "^2.2.0",
        "jsvat": "^2.5.3",
        "lodash": "^4.17.21",
        "mime-types": "^2.1.35",
        "minio": "^7.0.27",
        "nodemailer": "^6.7.3",
        "qrcode": "^1.5.0",
        "rambdax": "^8.0.1",
        "redis": "^3.1.2",
        "reflect-metadata": "^0.1.13",
        "server-timing": "^3.3.1",
        "sharp": "^0.30.4",
        "soap": "^0.43.0",
        "ts-node": "^10.7.0",
        "tslib": "^2.3.1",
        "type-graphql": "^1.1.1",
        "typegraphql-prisma": "^0.20.1",
        "typescript": "^4.6.3",
        "zod": "^3.14.4"
    },
    "devDependencies": {
        "@types/connect-redis": "^0.0.18",
        "@types/content-disposition": "^0.5.4",
        "@types/cookie": "^0.5.0",
        "@types/express-session": "^1.17.4",
        "@types/graphql-fields": "^1.3.4",
        "@types/graphql-upload": "^8.0.11",
        "@types/html-minifier": "^4.0.2",
        "@types/lodash": "^4.14.182",
        "@types/mime-types": "^2.1.1",
        "@types/minio": "^7.0.13",
        "@types/nodemailer": "^6.4.4",
        "@types/qrcode": "^1.4.2",
        "@types/sax": "^1.2.4",
        "@types/sharp": "^0.30.2",
        "@types/validator": "^13.7.2",
        "eslint": "^8.13.0",
        "prisma": "~3.12.0",
        "ts-node-dev": "^1.1.8",
        "type-fest": "^2.12.2"
    }
}

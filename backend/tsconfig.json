{
    "compilerOptions": {
        "esModuleInterop": true,
        "sourceMap": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "target": "esNext",
        "module": "commonjs",
        "lib": [
            "es2018",
            "es2021",
            "dom",
            "esnext.asynciterable"
        ],
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "skipLibCheck": true
    }
}

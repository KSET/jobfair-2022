        "@nuxtjs/eslint-config-typescript": "^8.0.0",
        "@nuxtjs/eslint-module": "^3.0.2",
        "@nuxtjs/stylelint-module": "^4.1.0",
        "@types/cssesc": "^3.0.0",
        "@types/http-status-codes": "^1.2.0",
        "@types/js-cookie": "^3.0.1",
        "@types/lodash-es": "^4.17.5",
        "@urql/core": "^2.3.6",
        "@vue/compiler-sfc": "^3.2.27",
        "@vueuse/nuxt": "^7.5.3",
        "cssesc": "^3.0.0",
        "eslint": "^8.7.0",

        "pinia": "^2.0.9",
        "postcss": "^8.4.5",
        "primevue": "^3.11.0",
        "vue": "^3.2.27"
    },
    "lint-staged": {
        "*.{ts,js,vue}": "eslint --cache",
        "*.{scss,css}": "stylelint"
    },
    "devDependencies": {
        "@fontsource/raleway": "^4.5.0",
        "@frsource/vite-plugin-stylelint": "^0.0.7",
        "@graphql-codegen/cli": "^2.3.1",
        "@graphql-codegen/introspection": "^2.1.1",

        "short-unique-id": "^4.4.4",
        "stylelint": "^14.2.0",
        "stylelint-config-recommended-vue": "^1.1.0",
        "stylelint-config-standard": "^24.0.0",
        "stylelint-config-standard-scss": "^3.0.0",
        "stylelint-scss": "^4.1.0",
        "type-fest": "^2.10.0",
        "typescript": "^4.5.4",
        "unplugin-icons": "^0.13.0",
        "vite-svg-loader": "^3.1.2",

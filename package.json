{
    "private": true,
    "scripts": {
        "dev": "nuxi dev",
        "build": "yarn graphql:schema:gen && nuxi build",
        "start": "node .output/server/index.mjs",
        "lint:js": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore",
        "lint:style": "stylelint \"**/*.{scss,css,vue}\" --ignore-path .gitignore",
        "prepare": "husky install",
        "lint": "yarn lint:js . && yarn lint:style",
        "cache:clear": "rimraf .nuxt/ .output/ node_modules/.cache/",
        "graphql:schema:dump": "cd ./backend && yarn graphql:schema:dump",
        "graphql:schema:gen": "graphql-codegen --config codegen.yml"
    },
    "dependencies": {
        "@nuxtjs/composition-api": "^0.31.0",
